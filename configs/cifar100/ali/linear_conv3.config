dataset cifar100
data_path ./data/cifar100
nclass 100
batchsize 128
optim sgd
lr 1e-1
wd 5e-5
niter 50000
stepsize 10000
mode full
fnet_path ./models/pretrained/cifar100/ali/fnet1.pt
hnet_path ./models/pretrained/cifar100/ali/ntk_hnet1.pt
clf_path ./models/pretrained/cifar100/ali/ntk_clf1.pt
model_path ./models/benchmark/cifar100/ali/linear_conv3.net
freeze_hnet [1, 2, 3]
linearize_hnet 3
linearize_clf